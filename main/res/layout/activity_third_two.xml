<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/all"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/round5"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/topmenu"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="8"
        >

        <LinearLayout

            android:id="@+id/top_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:background="#FFBB00">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.2"
                android:background="#FFBB00"
                tools:ignore="Suspicious0dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/home"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:textAlignment="center"
                        android:background="@drawable/home"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="6dp"
                        />

                    <ImageView
                        android:id="@+id/icon"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="3dp"
                        android:layout_marginBottom="3dp"
                        android:layout_weight="1"
                        android:src="@drawable/title" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:visibility="invisible"
                        android:singleLine="true"
                        android:text="NULL"
                        android:textColor="#ffffff"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/tab_right"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4">

                </LinearLayout>

                <Button
                    android:id="@+id/previous"
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/left"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="6dp"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:layout_weight="5.5"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5.7">
                </LinearLayout>

                <Button
                    android:textColor="#000000"
                    android:visibility="invisible"
                    android:id="@+id/play"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/listen"
                    android:textAlignment="center"
                    android:layout_weight="5.5"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="6dp"
                    android:textStyle="bold"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5.7">
                </LinearLayout>

                <Button
                    android:id="@+id/next"
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/right"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="6dp"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:layout_weight="5.5"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5.7">
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            >


            <FrameLayout
                android:id="@+id/inner"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFB"
                >


                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:orientation="horizontal"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_marginTop="5dip"
                        android:layout_margin="4dip"
                        >

                        <ImageButton
                            android:id="@+id/card01"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card02"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card03"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card04"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card05"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card06"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                    </LinearLayout>
                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:orientation="horizontal"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_margin="4dip"
                        >
                        <ImageButton
                            android:id="@+id/card07"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card08"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card09"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card10"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card11"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card12"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                    </LinearLayout>
                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:orientation="horizontal"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_margin="4dip"
                        >
                        <ImageButton
                            android:id="@+id/card13"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card14"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card15"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card16"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card17"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                        <ImageButton
                            android:id="@+id/card18"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:layout_margin="4dip"
                            />
                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>


        </FrameLayout>


    </LinearLayout>
</LinearLayout>